package com.slacks.service;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;

import com.slacks.dao.FileDAO;
import com.slacks.utils.FileBoardList;

@Service("fileService")
public class FileServiceImpl implements FileService{
	@Resource(name = "fileDAO")
	private FileDAO fileDAO;
	
	@Override
	public String fileBoardList(FileBoardList fileBoardList) throws Exception{
		String directory;
		directory = fileDAO.findBoardList(fileBoardList);
		return directory;
	}
}
