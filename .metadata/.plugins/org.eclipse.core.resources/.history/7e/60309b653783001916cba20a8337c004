<mapper namespace="file">
	<select id="selectByEmailFind" resultType="com.slacks.vo.UserVO">
        <![CDATA[
          SELECT
              email
          FROM
              users
          WHERE
              email = #{email}
      ]]>
	</select>
	
	<select id="findDirectory">
		<![CDATA[
			SELECT
				path
			FROM
				files
			WHERE
				#
		]]>
	</select>
</mapper>